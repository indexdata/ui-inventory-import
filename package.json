{
  "name": "@indexdata/inventory-import",
  "version": "4.0.0",
  "description": "Admin console for the Index Data Harvester",
  "repository": "https://github.com/indexdata/ui-inventory-import",
  "license": "Apache-2.0",
  "main": "src/index.js",
  "scripts": {
    "start": "stripes serve --startProxy --port 3003 --okapi https://snapshot-dev-okapi.folio-dev.indexdata.com --tenant harvester",
    "test": "jest",
    "minerva3": "stripes serve --port 3004 --okapi https://okapi.gbv.de --tenant minerva3",
    "lint": "eslint -f unix .",
    "qlint": "eslint -f unix  --rule '{\"no-console\":\"off\"}' .",
    "build-mod-descriptor": "stripes mod descriptor --full --strict | jq '.[]' > module-descriptor.json",
    "formatjs-compile": "formatjs compile-folder --ast --format simple ./translations/ui-inventory-import ./translations/ui-inventory-import/compiled"
  },
  "stripes": {
    "actsAs": [
      "app",
      "settings"
    ],
    "displayName": "ui-inventory-import.meta.title",
    "route": "/invimp",
    "queryResource": "query",
    "icons": [
      {
        "name": "app",
        "alt": "Admin console for the Index Data Harvester",
        "title": "Harvester admin"
      }
    ],
    "okapiInterfaces": {
    },
    "permissionSets": [
      {
        "permissionName": "module.inventory-import.enabled",
        "displayName": "UI: Inventory import module is enabled",
        "visible": false
      },
      {
        "permissionName": "settings.inventory-import.enabled",
        "displayName": "Settings (Inventory import): display list of settings pages",
        "subPermissions": [
          "settings.enabled"
        ],
        "visible": false
      },
      {
        "permissionName": "ui-inventory-import.settings.view",
        "displayName": "Harvester admin (Settings): View transformation pipelines and transformation steps, and log-file deletion threshold",
        "subPermissions": [
          "settings.inventory-import.enabled",
          "mod-settings.entries.collection.get",
          "inventory-update.import.steps.collection.get",
          "inventory-update.import.steps.item.get",
          "mod-settings.global.read.mod-inventory-update"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-inventory-import.settings.view-edit",
        "displayName": "Harvester admin (Settings): View, edit transformation pipelines and transformation steps, and log-file deletion threshold",
        "subPermissions": [
          "ui-inventory-import.settings.view",
          "mod-settings.entries.item.put",
          "mod-settings.entries.item.post",
          "inventory-update.import.steps.item.put",
          "mod-settings.global.write.mod-inventory-update"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-inventory-import.settings.view-edit-create",
        "displayName": "Harvester admin (Settings): View, edit, create transformation pipelines and transformation steps",
        "subPermissions": [
          "ui-inventory-import.settings.view-edit",
          "inventory-update.import.steps.item.post"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-inventory-import.settings.view-edit-create-delete",
        "displayName": "Harvester admin (Settings): View, edit, create, delete transformation pipelines and transformation steps",
        "subPermissions": [
          "ui-inventory-import.settings.view-edit-create",
          "inventory-update.import.steps.item.delete"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-inventory-import.harvestables.view",
        "displayName": "Harvester admin: View harvestables",
        "subPermissions": [
          "module.inventory-import.enabled",
          "xxx-harvester-admin.harvestables.collection.get",
          "xxx-harvester-admin.harvestables.item.get",
          "xxx-harvester-admin.harvestables.failed-records.collection.get",
          "xxx-harvester-admin.harvestables.log.get",
          "xxx-harvester-admin.previous-jobs.collection.get"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-inventory-import.harvestables.view-edit",
        "displayName": "Harvester admin: View, edit harvestables, start/stop jobs",
        "subPermissions": [
          "ui-inventory-import.harvestables.view",
          "xxx-harvester-admin.transformations.collection.get",
          "xxx-harvester-admin.harvestables.item.put",
          "xxx-harvester-admin.run-jobs",
          "xxx-harvester-admin.stop-jobs"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-inventory-import.harvestables.view-edit-create",
        "displayName": "Harvester admin: View, edit, create harvestables, start/stop jobs",
        "subPermissions": [
          "ui-inventory-import.harvestables.view-edit",
          "xxx-harvester-admin.harvestables.item.post"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-inventory-import.harvestables.view-edit-create-delete",
        "displayName": "Harvester admin: View, edit, create, delete harvestables, start/stop jobs",
        "subPermissions": [
          "ui-inventory-import.harvestables.view-edit-create",
          "xxx-harvester-admin.harvestables.item.delete"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-inventory-import.jobs-and-failed-records.view",
        "displayName": "Harvester admin: View jobs and failed records",
        "subPermissions": [
          "module.inventory-import.enabled",
          "xxx-harvester-admin.previous-jobs.collection.get",
          "xxx-harvester-admin.previous-jobs.item.get",
          "xxx-harvester-admin.previous-jobs.log.get",
          "xxx-harvester-admin.previous-jobs.failed-records.collection.get",
          "xxx-harvester-admin.previous-jobs.failed-records.item.get"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-inventory-import.all",
        "displayName": "Harvester admin: All permissions",
        "subPermissions": [
          "module.inventory-import.enabled",
          "settings.inventory-import.enabled",
          "ui-inventory-import.settings.view",
          "ui-inventory-import.settings.view-edit",
          "ui-inventory-import.settings.view-edit-create",
          "ui-inventory-import.settings.view-edit-create-delete",
          "ui-inventory-import.harvestables.view",
          "ui-inventory-import.harvestables.view-edit",
          "ui-inventory-import.harvestables.view-edit-create",
          "ui-inventory-import.harvestables.view-edit-create-delete",
          "ui-inventory-import.jobs-and-failed-records.view"
        ],
        "visible": true
      }
    ]
  },
  "dependencies": {
    "@axetroy/react-download": "^1.1.1",
    "@indexdata/oai-pmh": "^0.0.2",
    "final-form-arrays": "^3.0.2",
    "final-form-set-field-data": "^1.0.2",
    "lodash": "^4.17.0",
    "query-string": "^6.0.0",
    "react-final-form": "^6.3.0",
    "react-final-form-arrays": "^3.1.3",
    "react-inspector": "^5.1.0",
    "use-deep-compare": "^1.1.0"
  },
  "peerDependencies": {
    "@folio/stripes": "^10.0.0",
    "prop-types": "*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "^7.1.5",
    "react-router": "*",
    "react-router-dom": "*"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.15.0",
    "@folio/eslint-config-stripes": "^8.0.0",
    "@folio/jest-config-stripes": "^3.0.0",
    "@folio/stripes": "^10.0.0",
    "@folio/stripes-cli": "^4.0.0",
    "@formatjs/cli": "^4.2.20",
    "eslint": "^7.32.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "^7.1.5",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0"
  }
}
